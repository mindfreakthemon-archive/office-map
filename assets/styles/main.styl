* {
	margin: 0;
	padding: 0;
}

body {
	background: #eee;
	padding-top: 70px;
}

#map {
  position absolute;
  left 0;
  top: 0;
  width: 100vw;
  height: 100vh;
}

.bold {
	font-weight: bold;
	font-size: 1.1em;
}

.action {
	cursor: pointer;
}

admin,
toolbar,
admin-tools {
	display: block;
}

admin {
	margin-top: 50px;
}

.pagination {
	a {
		cursor: pointer;
	}

	.current {
		display: none;
	}
}
